     ]
    }
   ],
   "source": [
    "num = float(input(\"Enter a number: \"))\n",
    "guess = num / 2\n",
    "\n",
    "for i in range(10):\n",
    "    guess = (guess + num / guess) / 2\n",
    "\n",
    "print(\"Square root =\", guess)\n"
   ]
