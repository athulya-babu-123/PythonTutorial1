   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Enter binary number:  1021\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Decimal value = 13\n"
     ]
    }
   ],
   "source": [
    "binary = input(\"Enter binary number: \")\n",
    "decimal = 0\n",
    "power = 0\n",
    "\n",
    "for digit in reversed(binary):\n",
    "    decimal += int(digit) * (2 ** power)\n",
    "    power += 1\n",
    "\n",
    "print(\"Decimal value =\", decimal)\n"
   ]
