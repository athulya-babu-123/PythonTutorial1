   "source": [
    "## palindrome"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "0f17ad5c-589f-49aa-ab56-9128bf55a454",
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Enter a string:  Palindrome\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "String is Not Symmetrical\n",
      "String is Not Palindrome\n"
     ]
    }
   ],
   "source": [
    "s = input(\"Enter a string: \")\n",
    "\n",
    "mid = len(s) // 2\n",
    "\n",
    "if s[:mid] == s[mid:]:\n",
    "    print(\"String is Symmetrical\")\n",
    "else:\n",
    "    print(\"String is Not Symmetrical\")\n",
    "\n",
    "if s == s[::-1]:\n",
    "    print(\"String is Palindrome\")\n",
    "else:\n",
    "    print(\"String is Not Palindrome\")\n"
   ]
  },
